@model IEnumerable<ProjetoVeterinaria.Models.clTipo>

@{
    ViewBag.Title = "Listar Tipo";
}

<h2>Lista de Tipos</h2>

<p>
    @Html.ActionLink("Novo", "CadTipo", "Tipo")
</p>
<table class="table text-center">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.codTipo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tipo)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.codTipo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tipo)
            </td>
        </tr>
    }

</table>
